<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
                android:shape="rectangle">
            <gradient
                    android:angle="180"
                    android:endColor="@color/side_bar_3"
                    android:startColor="@color/side_bar_2" />
        </shape>
    </item>
    <item
            android:bottom="2dp"
            android:left="2dp"
            android:right="2dp"
            android:top="2dp">
        <shape
                android:shape="rectangle">

            <solid android:color="@color/projeto_2" />

        </shape>
    </item>
</layer-list>